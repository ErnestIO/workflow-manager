{
    "networks": {
        "items":[
            {
                "network_aws_id": "network-1-id",
                "name": "network-1"
            },
            {
                "network_aws_id": "network-2-id",
                "name": "network-2"
            }
        ]
    },
    "instances": {
        "items":[
            {
                "id": "instance-id",
                "name": "instance",
                "network": "network-1",
